class Solution:
    def kidsWithCandies(self, candies: List[int], extraCandies: int) -> List[bool]:
        res=[]
        maxnum=max(candies)
        for i in range(len(candies)):
            a=candies[i] + extraCandies
            if a >= maxnum:
                res.append(True)
            else:
                res.append(False)
                print(res)
        return res
        
        
